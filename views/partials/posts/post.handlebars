<div class="text-center picContainer">
    {{#if bigPic}}
        <img class="img-fluid rounded postPic" data-small={{dataValues.url}} data-fixed-small={{dataValues.urlOriginalStill}} data-fixed={{dataValues.urlStill}} src={{dataValues.urlOriginal}}>
    {{else}}
         <img class="img-fluid rounded postPic" data-original={{dataValues.urlOriginal}} data-fixed-small={{dataValues.urlOriginalStill}} data-fixed={{dataValues.urlStill}} src={{dataValues.url}}>
    {{/if}}
    <div class="overlay">
        <button class="btn btn-secondary likeBtn" data-postId={{dataValues.id}}>
           <i class="far fa-thumbs-up"></i> Like
        </button>
        <button class="btn btn-warning shareBtn" data-toggle="modal" data-target="#shareModal">
            <i class="fas fa-share-alt"></i> Share
        </button>
    </div>
</div>