{{> navs/navBar user='active'}} 
<h2 class="text-secondary">{{userName}} <button id="followBtn" data-id={{userId}} class="btn btn-primary"><i class="fas fa-user-plus"></i>Follow</button></h2>
<div class="row justify-content-left">
    {{#each posts}}
        <div class="col-lg-3 col-md-6">
            <a href="/post/{{dataValues.id}}">
                {{> posts/post }}
            </a>
        </div>
    {{/each}}
</div>